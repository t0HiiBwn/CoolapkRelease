    <View android:id="@+id/tt_dislike_line1" android:background="#0f161823" android:visibility="visible" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginLeft="4dp" android:layout_marginTop="6dp" android:layout_marginRight="4dp" android:layout_marginBottom="11.5dp"/>
    <TextView android:textSize="12sp" android:textColorHint="#80161823" android:id="@+id/tt_edit_suggestion" android:background="@drawable/tt_comment_tv" android:paddingLeft="8.6dp" android:paddingTop="10dp" android:paddingBottom="10dp" android:focusable="false" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:hint=" 其他建议" android:drawableLeft="@drawable/tt_suggestion_logo"/>
</LinearLayout>
