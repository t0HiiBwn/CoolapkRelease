<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/tt_native_video_titlebar" android:background="@color/tt_titlebar_background_ffffff" android:layout_width="match_parent" android:layout_height="44dp">
        <ImageView android:id="@+id/tt_titlebar_back" android:paddingLeft="5dp" android:paddingTop="5dp" android:paddingRight="5dp" android:paddingBottom="5dp" android:focusable="true" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/tt_leftbackicon_selector" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:layout_alignParentStart="true"/>
        <ImageView android:id="@+id/tt_titlebar_close" android:paddingLeft="5dp" android:paddingTop="5dp" android:paddingRight="5dp" android:paddingBottom="5dp" android:focusable="true" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/tt_titlebar_close_seletor" android:layout_toRightOf="@+id/tt_titlebar_back" android:layout_centerVertical="true" android:layout_toEndOf="@+id/tt_titlebar_back"/>
        <TextView android:textSize="17sp" android:textColor="#222222" android:ellipsize="marquee" android:gravity="center" android:id="@+id/tt_titlebar_title" android:layout_width="200dp" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:singleLine="true" android:layout_toRightOf="@+id/tt_titlebar_close" android:layout_centerVertical="true" android:layout_marginStart="5dp" android:layout_toEndOf="@+id/tt_titlebar_close"/>
        <TextView android:textColor="@android:color/tab_indicator_text" android:ellipsize="marquee" android:gravity="right" android:id="@+id/tt_titlebar_dislike" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="15dp" android:text="反馈" android:singleLine="true" android:layout_toRightOf="@+id/tt_titlebar_title" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:layout_toEndOf="@+id/tt_titlebar_title" android:layout_alignParentEnd="true"/>
    </RelativeLayout>
    <com.bytedance.sdk.openadsdk.core.widget.TTScrollView android:id="@+id/tt_scroll_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
            <FrameLayout android:id="@+id/tt_native_video_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="200dp" android:scaleType="centerCrop"/>
            <LinearLayout android:orientation="vertical" android:id="@+id/web_frame" android:layout_width="match_parent" android:layout_height="wrap_content">
                <RelativeLayout android:id="@+id/tt_rl_download" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp">
                    <com.bytedance.sdk.openadsdk.core.widget.RoundImageView android:id="@+id/tt_video_ad_logo_image" android:visibility="gone" android:layout_width="44dp" android:layout_height="44dp" android:layout_marginLeft="5dp" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_centerInParent="true" android:layout_centerHorizontal="true" android:layout_marginStart="5dp" android:layout_alignParentStart="true"/>
                    <TextView android:textSize="19sp" android:textStyle="bold" android:textColor="@color/tt_white" android:gravity="center" android:id="@+id/tt_video_btn_ad_image_tv" android:background="@drawable/tt_circle_solid_mian" android:visibility="visible" android:layout_width="44dp" android:layout_height="44dp" android:layout_marginLeft="5dp" android:layout_alignParentLeft="true" android:layout_centerInParent="true" android:layout_marginStart="5dp" android:layout_alignParentStart="true"/>
                    <TextView android:textSize="12sp" android:textColor="@color/tt_ssxinmian11" android:ellipsize="end" android:id="@+id/tt_video_ad_name" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="6dp" android:layout_marginRight="80dp" android:maxLines="1" android:layout_toRightOf="@+id/tt_video_btn_ad_image_tv" android:layout_centerInParent="true" android:layout_marginStart="5dp" android:layout_marginEnd="80dp" android:layout_toEndOf="@+id/tt_video_btn_ad_image_tv"/>
                    <TextView android:textSize="14sp" android:textColor="@color/tt_white" android:id="@+id/tt_video_ad_button" android:background="@drawable/tt_ad_cover_btn_begin_bg" android:padding="2dp" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5dp" android:text="@string/tt_video_download_apk" android:layout_alignParentRight="true" android:layout_centerInParent="true" android:layout_marginEnd="5dp" android:layout_alignParentEnd="true"/>
                </RelativeLayout>
                <FrameLayout android:layout_width="match_parent" android:layout_height="match_parent">
                    <com.bytedance.sdk.openadsdk.core.widget.SSWebViewVideoPage android:id="@+id/tt_browser_webview" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="2dp"/>
                    <ViewStub android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:id="@+id/tt_browser_download_btn_stub" android:layout="@layout/tt_browser_download_layout" android:layout_width="match_parent" android:layout_height="wrap_content"/>
                    <ProgressBar android:layout_gravity="top|center_vertical|center_horizontal|center" android:id="@+id/tt_browser_progress" android:layout_width="match_parent" android:layout_height="3dp" android:layout_marginTop="2dp" android:progress="1" android:progressDrawable="@drawable/tt_browser_progress_style" style="@android:style/Widget.ProgressBar.Horizontal"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </com.bytedance.sdk.openadsdk.core.widget.TTScrollView>
</LinearLayout>
