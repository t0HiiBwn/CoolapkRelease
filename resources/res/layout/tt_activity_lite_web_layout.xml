<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/tt_white" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:gravity="center_vertical" android:layout_width="match_parent" android:layout_height="50dp">
        <ImageView android:id="@+id/tt_lite_web_back" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:src="@drawable/tt_lefterbackicon_titlebar"/>
        <TextView android:textSize="16sp" android:textColor="#030303" android:id="@+id/tt_lite_web_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <View android:background="#15161823" android:layout_width="match_parent" android:layout_height="0.5px"/>
    <com.bytedance.sdk.openadsdk.core.widget.webview.SSWebView android:id="@+id/tt_lite_web_view" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
</LinearLayout>
