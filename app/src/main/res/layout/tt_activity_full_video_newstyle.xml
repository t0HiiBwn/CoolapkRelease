<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/tt_reward_root" android:background="@android:color/black" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/tt_click_upper_non_content_layout" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_above="@+id/tt_video_reward_container"/>
    <RelativeLayout android:gravity="center" android:id="@+id/tt_video_reward_bar" android:layout_width="280dp" android:layout_height="70dp" android:layout_marginBottom="33dp" android:layout_above="@+id/tt_video_reward_container" android:layout_centerHorizontal="true">
        <com.bytedance.sdk.openadsdk.core.widget.TTRoundRectImageView android:id="@+id/tt_reward_ad_icon" android:background="@android:color/transparent" android:layout_width="69dp" android:layout_height="69dp" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:layout_alignParentStart="true"/>
        <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toRightOf="@+id/tt_reward_ad_icon" android:layout_toEndOf="@+id/tt_reward_ad_icon">
            <TextView android:textSize="17sp" android:textColor="@android:color/white" android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/tt_reward_ad_appname" android:layout_width="wrap_content" android:layout_height="27dp" android:layout_marginLeft="14dp" android:maxWidth="176dp" android:text="绝地求生刺激战场长城" android:singleLine="true" android:layout_marginStart="14dp"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="14dp" android:layout_marginTop="10dp" android:layout_marginStart="14dp">
                <com.bytedance.sdk.openadsdk.core.widget.TTRatingBar android:layout_gravity="center" android:id="@+id/tt_rb_score" android:layout_width="wrap_content" android:layout_height="14dp"/>
                <TextView android:textSize="13sp" android:textColor="@android:color/white" android:ellipsize="end" android:id="@+id/tt_comment_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:maxWidth="72dp" android:text="@string/tt_comment_num" android:singleLine="true"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    <FrameLayout android:layout_gravity="center" android:id="@+id/tt_video_reward_container" android:background="#000000" android:layout_width="match_parent" android:layout_height="211dp" android:scaleType="centerCrop" android:layout_centerInParent="true"/>
    <FrameLayout android:id="@+id/tt_click_lower_non_content_layout" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/tt_video_reward_container"/>
    <TextView android:textSize="15sp" android:textColor="@color/tt_white" android:gravity="center" android:id="@+id/tt_reward_ad_download" android:background="@drawable/tt_reward_video_download_btn_bg" android:layout_width="260dp" android:layout_height="36dp" android:layout_marginTop="70dp" android:text="@string/tt_video_download_apk" android:layout_below="@+id/tt_video_reward_container" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
    <com.bytedance.sdk.openadsdk.core.widget.webview.SSWebView android:id="@+id/tt_reward_browser_webview" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent" android:layerType="hardware"/>
    <com.bytedance.sdk.openadsdk.core.widget.webview.SSWebView android:id="@+id/tt_browser_webview_loading" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/tt_reward_full_endcard_backup" android:background="@color/tt_white" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.bytedance.sdk.openadsdk.core.widget.TTRoundRectImageView android:id="@+id/tt_reward_ad_icon_backup" android:background="@android:color/transparent" android:layout_width="100dp" android:layout_height="100dp" android:layout_marginTop="172dp" android:layout_centerVertical="true"/>
        <TextView android:textSize="20sp" android:textColor="#ff333333" android:ellipsize="end" android:id="@+id/tt_reward_ad_appname_backup" android:layout_width="wrap_content" android:layout_height="28dp" android:layout_marginTop="16dp" android:maxWidth="180dp" android:text="部落战争：皇室冲突" android:singleLine="true"/>
        <com.bytedance.sdk.openadsdk.core.widget.TTRatingBar android:layout_gravity="center" android:id="@+id/tt_rb_score_backup" android:layout_width="wrap_content" android:layout_height="16dp" android:layout_marginTop="12dp"/>
        <TextView android:textSize="14sp" android:textColor="#ff93959a" android:ellipsize="end" android:id="@+id/tt_comment_backup" android:layout_width="wrap_content" android:layout_height="20dp" android:layout_marginTop="8dp" android:text="@string/tt_comment_num_backup" android:singleLine="true"/>
        <TextView android:textSize="16sp" android:textColor="@color/tt_white" android:gravity="center" android:id="@+id/tt_reward_ad_download_backup" android:background="@drawable/tt_reward_full_video_backup_btn_bg" android:layout_width="328dp" android:layout_height="46dp" android:layout_marginTop="190dp" android:text="@string/tt_video_download_apk"/>
    </LinearLayout>
    <com.bytedance.sdk.openadsdk.core.widget.PlayableLoadingView android:id="@+id/tt_reward_playable_loading" android:focusable="true" android:clickable="true" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <FrameLayout android:gravity="center_vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.bytedance.sdk.openadsdk.component.reward.top.TopProxyLayout android:id="@+id/tt_top_layout_proxy" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </FrameLayout>
    <RelativeLayout android:gravity="center" android:id="@+id/tt_video_ad_close_layout" android:background="@drawable/tt_mute_btn_bg" android:visibility="gone" android:layout_width="28dp" android:layout_height="28dp" android:layout_marginTop="20dp" android:layout_marginRight="24dp" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:layout_marginEnd="24dp">
        <ImageView android:id="@+id/tt_video_ad_close" android:visibility="gone" android:layout_width="12dp" android:layout_height="12dp" android:src="@drawable/tt_video_close_drawable"/>
    </RelativeLayout>
    <TextView android:textSize="7sp" android:textColor="#b7b7b7" android:gravity="center" android:id="@+id/tt_ad_logo" android:paddingLeft="2dp" android:layout_width="32dp" android:layout_height="12dp" android:layout_marginLeft="16dp" android:layout_marginBottom="20dp" android:text="@string/tt_ad_logo_txt" android:drawableLeft="@drawable/tt_ad_logo_reward_full" android:layout_alignParentLeft="true" android:layout_alignParentBottom="true" android:drawableStart="@drawable/tt_ad_logo_reward_full" android:paddingStart="2dp"/>
</RelativeLayout>
