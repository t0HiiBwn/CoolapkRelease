<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/tt_splash_express_container" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <FrameLayout android:id="@+id/tt_splash_video_container" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <com.bytedance.sdk.openadsdk.core.widget.gif.GifView android:layout_gravity="center" android:id="@+id/tt_splash_ad_gif" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitXY"/>
